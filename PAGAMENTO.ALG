Algoritmo "pagamento"

Var
nome:caractere
valor_hora,horas_trabalhadas,pagamento:real


Inicio
escreva("qual seu nome?")
leia(nome)
escreva("qual valor da hora trabalhada?")
leia(valor_hora)
escreva("quantas horas trabalhadas?")
leia(horas_trabalhadas)
pagamento<-valor_hora*horas_trabalhadas
escreval("o vlaor a ser recebido por você ",nome," é de ", pagamento)

Fimalgoritmo